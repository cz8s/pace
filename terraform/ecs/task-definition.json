[{
    "name": "pace",
    "image": "lplotni/pace-app",
    "cpu": 1024,
    "memory": 512,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "eu-central-1",
        "awslogs-create-group": "true",
        "awslogs-group": "pace-logs"
      }
    },
    "portMappings": [{
      "containerPort": 3000,
      "hostPort": 3000
    }],
    "essential": true,
    "command": ["/usr/local/bin/npm", "start"]
  }

]
